{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tinywow-search",
  "title": "TinyWow Search",
  "description": "Search tools of tinywow.com and open in your browser",
  "icon": "favicon-tinywow.png",
  "author": "shikibu9419",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Tinywow",
      "description": "Select and open TinyWow tool in browser",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.3",
    "dom-parser": "^0.1.6",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/dom-parser": "^0.1.1",
    "@types/node": "18.8.3",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "update-tools": "./scraping/update_tools && npm run build",
    "publish": "ray publish"
  }
}
